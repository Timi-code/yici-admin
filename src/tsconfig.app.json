{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "types": [
      "node"
    ],
    "baseUrl": "../",
    "paths": {
      "@models/*": [
        "./src/app/models/*"
      ],
      "@services/*": [
        "./src/app/services/*"
      ],
      "@share/*": [
        "./src/app/share/*"
      ]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}